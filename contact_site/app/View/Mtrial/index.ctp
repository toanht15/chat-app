    <script type="text/javascript">
      //モーダル画面
      function openTermsOfService(){
        document.getElementById('top').style.overflow  = "hidden";
        $.ajax({
          type: 'post',
          dataType: 'html',
          cache: false,
          url: "<?= $this->Html->url(['controller' => 'Mtrial', 'action' => 'remoteTermsOfService']) ?>",
          success: function(html){
            modalOpen.call(window, html, 'p-show-terms-of-service', '利用規約', 'moment');
          }
        });
      }

      //登録する
      function saveAct(){
        var validate = 'true';
        if($('#MCompanyCompanyName').val() == "") {
          document.getElementById('companyName').style.display = "block";
          validate = 'false';
        }
        if($('#MAgreementsApplicationName').val() == "") {
          document.getElementById('name').style.display = "block";
          validate = 'false';
        }
        if($('#ContractUserMailAddress').val() == "") {
          document.getElementById('mailAddress').style.display = "block";
          document.getElementById('mailFormat').style.display = "none";
          document.getElementById('empty').style.display = "block";
          document.getElementById('registered').style.display = "none";
          validate = 'false';
        }
        if($('#ContractUserMailAddress').val() !== "" && $('#ContractUserMailAddress').val().match(/.+@.+\..+/)==null){
          document.getElementById('mailAddress').style.display = "block";
          document.getElementById('empty').style.display = "none";
          document.getElementById('mailFormat').style.display = "block";
          document.getElementById('registered').style.display = "none";
          validate = 'false';
        }
        if($('#MAgreementsTelephoneNumber').val() == "") {
          document.getElementById('phoneNumber').style.display = "block";
          validate = 'false';
        }
        if(!$("#agree").prop('checked')) {
          document.getElementById('agreeEroor').style.display = "block";
          validate = 'false';
        }
        if(validate == 'false') {
          return false;
        }
        //loading画像
        loading.load.start();

        $.ajax({
          type: "POST",
          url: "<?= $this->Html->url('/Mtrial/add') ?>",
          data: $('#ContractAddForm').serialize()
        }).done(function(data){
          if(data.trim() == 'error') {
            loading.load.finish();
            document.getElementById('mailAddress').style.display = "block";
            document.getElementById('empty').style.display = "none";
            document.getElementById('mailFormat').style.display = "none";
            document.getElementById('registered').style.display = "block";
           }
          else {
            location.href = "<?= $this->Html->url('/Mtrial/thanks') ?>";
          }
        }).fail(function(data){
          var obj = JSON.parse(data.responseText);
          alert(obj.message);
        });
      }

      window.onload = function () {
        //パスワード自動生成
        var str = random();
        $('#ContractUserPassword').val(str);
        document.form.data[user_password].value= str;
